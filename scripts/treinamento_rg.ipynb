      "id": "c5806942",
      "metadata": {},
      "outputs": [],
      "source": [
        "for col in ['color1_hex','color2_hex','color3_hex']:\n",
        "    X[col] = X[col].apply(lambda x: int(x.replace('#',''), 16))\n",
        "\n",
        "le = LabelEncoder()\n",
        "y = le.fit_transform(y)"
      ]
    },
